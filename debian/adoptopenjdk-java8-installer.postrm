#!/bin/sh

set -e

case "$1" in
  purge|remove|abort-install)
    rm -rf "/usr/lib/jvm/java-8-adoptopenjdk"
    rm -rf "/var/cache/adoptopenjdk-jdk8-installer"
    rm -rf "/usr/lib/adoptopenjdk-jdk8-installer-unpackdir"
    rm -rf "/usr/lib/jvm/.java-8-adoptopenjdk.jinfo"
    ;;
esac

#DEBHELPER#

exit 0

